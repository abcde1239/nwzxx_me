{
  "name": "nwzxx-me",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{ts,js,tsx,jsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,js,tsx,jsx}' --fix",
    "format": "prettier --write 'scr/**/*.{ts,tsx,js,jsx}' --fix",
    "check:format": "prettier --check 'scr/**/*/{ts,js,tsx,jsx,json,css,md}'",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tsconfig/node20": "^20.1.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "npm:rolldown-vite@7.1.12",
    "vue-tsc": "^3.0.7"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.12"
    }
  }
}
